{
  "test": ".ts",
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": false,
      "decorators": true,
      "dynamicImport": true,
      "topLevelAwait": true
    },
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"]
    },
    "target": "es2022"
  },
  "module": {
    "type": "nodenext"
  },
  "isModule": true
}
